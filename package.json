{
    "name": "express-prisma-news-api",
    "version": "0.0.1",
    "description": "Express Prisma News API",
    "author": "Erol Senol <erolsnl@gmail.com>",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/erolsenol/node-express-prisma-news-api.git"
    },
    "keywords": [
        "typescript",
        "express",
        "express.js",
        "prisma",
        "prisma.io",
        "news",
        "news-api"
    ],
    "homepage": "https://github.com/erolsenol/node-express-prisma-news-api",
    "bugs": {
        "url": "https://github.com/erolsenol/node-express-prisma-news-api/issues"
    },
    "engines": {
        "node": ">=14 <=21"
    },
    "scripts": {
        "start:dev": "npm run build && cross-env NODE_ENV=production node dist/server.js",
        "start": "nodemon dist/server.js cross-env NODE_ENV=production",
        "dev": "cross-env NODE_ENV=development nodemon",
        "build": "swc src -d dist --source-maps --copy-files --env-name='production'",
        "build:tsc": "tsc && tsc-alias",
        "test": "jest --forceExit --detectOpenHandles",
        "lint": "eslint --ignore-path .gitignore --ext .ts src/",
        "lint:fix": "npm run lint -- --fix",
        "prisma:init": "npx prisma init",
        "prisma:generate": "npx prisma generate",
        "prisma:migrate": "npx prisma migrate dev --preview-feature --create-only",
        "prisma:db-push": "npx prisma db push",
        "prisma:db-seed": "npx prisma db seed",
        "prisma:studio": "npx prisma studio",
        "prisma:deploy": "npx prisma migrate deploy",
        "pm2:prod": "npm run build && pm2 start ecosystem.config.js --only trendyol-property-fetch-3000",
        "docker": "docker-compose -f docker-compose.yml up"
    },
    "prisma": {
        "schema": "./src/prisma/schema.prisma",
        "seed": "ts-node ./src/prisma/seed.ts"
    },
    "dependencies": {
        "@getvim/execute": "^1.0.0",
        "@google-cloud/storage": "^7.7.0",
        "@prisma/client": "^5.10.2",
        "axios": "^1.6.7",
        "bcrypt": "^5.1.1",
        "bullmq": "^5.1.8",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "envalid": "^8.0.0",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "helmet": "^7.1.0",
        "hpp": "^0.2.3",
        "i": "^0.3.7",
        "ioredis": "^5.3.2",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "morgan": "^1.10.0",
        "node-cron": "^3.0.3",
        "npm": "^10.4.0",
        "puppeteer": "^21.11.0",
        "puppeteer-extra": "^3.3.6",
        "puppeteer-extra-plugin-anonymize-ua": "^2.4.6",
        "puppeteer-extra-plugin-stealth": "^2.11.2",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0",
        "url-slug": "^4.0.1",
        "winston": "^3.8.1",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "@swc/cli": "^0.1.63",
        "@swc/core": "^1.3.101",
        "@types/bcrypt": "^5.0.0",
        "@types/compression": "^1.7.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.5",
        "@types/hpp": "^0.2.2",
        "@types/jest": "^28.1.6",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/morgan": "^1.9.3",
        "@types/node": "^20.10.5",
        "@types/supertest": "^2.0.12",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.0",
        "node-config": "^0.0.2",
        "node-gyp": "^9.1.0",
        "nodemon": "^3.0.2",
        "pm2": "^5.3.0",
        "prettier": "^3.1.1",
        "prisma": "^5.10.2",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.8",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.3.3"
    },
    "main": "ecosystem.config.js"
}
